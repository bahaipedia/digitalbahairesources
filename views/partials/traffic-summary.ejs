<section id="traffic-summary">
    <h2>Summary for <span id="summary-month"></span>, <span id="summary-year"></span></h2>
    <table class="traffic-summary-table">
        <thead>
            <tr class="traffic-summary-header">
                <th class="header-unique-visitors">Unique Visitors</th>
                <th class="header-number-visits">Number of Visits</th>
                <th class="header-pages">Pages</th>
                <th class="header-hits">Hits</th>
                <th class="header-bandwidth">Bandwidth</th>
            </tr>
        </thead>
        <tbody>
            <tr class="traffic-summary-loading">
                <td colspan="5">Loading...</td>
            </tr>
        </tbody>
    </table>
</section>
